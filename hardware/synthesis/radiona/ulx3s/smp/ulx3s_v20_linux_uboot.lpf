LOCATE COMP "system_gpioA_gpio[12]" SITE "B8";  # J1_15- GN5
LOCATE COMP "system_gpioA_gpio[13]" SITE "C6";  # J1_17+ GP6
LOCATE COMP "gn[6]" SITE "C7";  # J1_17- GN6
