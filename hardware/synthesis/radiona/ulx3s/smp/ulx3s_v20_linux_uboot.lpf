LOCATE COMP "debug_jtag_tdo" SITE "A11"; # J1_7-  GN1 PCLK
LOCATE COMP "gp[2]" SITE "A9";  # J1_9+  GP2 GR_PCLK
LOCATE COMP "debug_jtag_tdi" SITE "B10"; # J1_9-  GN2 GR_PCLK
